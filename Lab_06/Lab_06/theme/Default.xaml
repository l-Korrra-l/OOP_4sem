<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:Lab_06.theme">
    <Style x:Key="GridTopBar" TargetType="Grid">
        <Setter Property="Background" Value="#FFBCCDDA"/>
    </Style>
    <Style x:Key="Menu" TargetType="MenuItem">
        <Setter Property="Background" Value="#FFBCCDDA"/>
    </Style>
    <Style x:Key="GridLeftBar" TargetType="Grid">
        <Setter Property="Background" Value="#FF7794A9"/>
    </Style>
    <Style x:Key="GridPrincipalTheme" TargetType="Grid">
        <Setter Property="Background" Value="#FFFDFDFF"/>
    </Style>
    <Style x:Key="CartBackground" TargetType="Grid">
        <Setter Property="Background" Value="#d8b9c3"/>
    </Style>
    <Style x:Key="CartSeparator" TargetType="Rectangle">
        <Setter Property="Fill" Value="#363062"/>
    </Style>
    <Style x:Key="Buttons" TargetType="Button">
        <Setter Property="Background" Value="#FFBCCDDA"/>
    </Style>

</ResourceDictionary>